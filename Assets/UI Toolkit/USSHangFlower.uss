    margin-right: 5px;
    margin-bottom: 5px;
    margin-left: 5px;
    font-size: 20px;
    white-space: normal;
    justify-content: flex-start;
    align-self: auto;
    align-items: auto;
    flex-direction: row-reverse;
    -unity-font-style: bold;
}

.setting-slider {
    min-height: 20px;
    max-width: 75%;
    min-width: 75%;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
    font-size: 25px;
    -unity-text-align: upper-center;
    -unity-font-style: bold;
}

.navigation-button {
}

.popup {
}

.popup-box {
    position: absolute;
    align-self: center;
    align-items: center;
    justify-content: space-around;
    height: 100%;
    width: 50%;
    left: 25%;
    background-color: rgba(0, 0, 0, 0.59);
    display: flex;
    visibility: visible;
    padding-top: 0;
    padding-bottom: 0;
    padding-left: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

.category {
}

.stat-panel {
    flex-grow: 1;
    height: 100%;
    max-height: 100%;
    min-height: 100%;
    width: 60%;
    background-color: rgb(207, 207, 207);
    min-width: 45%;
    max-width: 65%;
    background-image: none;
    -unity-background-scale-mode: scale-and-crop;
    position: absolute;
    transition-duration: 2s;
    transition-timing-function: ease-in-back;
    transition-property: position;
    left: -61%;
}

.stat-panel:enabled {
    transition-property: position;
    transition-duration: 2s;
    transition-timing-function: ease-out-back;
    left: 0;
}

.stat-panel:disabled {
    transition-property: position;
    transition-duration: 2s;
    transition-timing-function: ease-in-back;
    left: -61%;
}

.stat-label {
    font-size: 25px;
    -unity-text-align: upper-center;
}
